{
  "manifest_version": 2,
  "name": "Super Video Downloader",
  "version": "0.0.5",
  "description": "Super Video Downloader",
  "browser_action": {
    "default_title": "Super Video Downloader",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "activeTab",
    "https://api.hypstar.com/*",
    "http://p16-hypstarcdn-com.akamaized.net/*"
  ],
  "options_page": "options.html"
}
